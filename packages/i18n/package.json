{
	"name": "@rocket.chat/i18n",
	"version": "1.10.0",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"types": "./dist/esm/index.d.ts"
		},
		"./dist/resources": {
			"import": "./dist/resources.mjs",
			"require": "./dist/resources.js",
			"types": "./dist/resources.d.ts"
		},
		"./dist/languages": {
			"import": "./dist/languages.mjs",
			"require": "./dist/languages.js",
			"types": "./dist/languages.d.ts"
		}
	},
	"main": "./dist/cjs/index.js",
	"types": "./dist/esm/index.d.ts",
	"files": [
		"/dist"
	],
	"scripts": {
		".:build:cjs": "tsc -p tsconfig.cjs.json",
		".:build:esm": "tsc -p tsconfig.esm.json",
		"build": "node --experimental-transform-types ./src/scripts/build.mts && run .:build:esm && run .:build:cjs",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "jest",
		"testunit": "jest"
	},
	"devDependencies": {
		"@rocket.chat/jest-presets": "workspace:~",
		"@rocket.chat/tools": "workspace:~",
		"@rocket.chat/tsconfig": "workspace:*",
		"eslint": "~8.45.0",
		"i18next": "~23.4.9",
		"jest": "~30.0.5",
		"typescript": "~5.9.2"
	},
	"peerDependencies": {
		"@rocket.chat/tools": "workspace:~",
		"i18next": "*"
	},
	"volta": {
		"extends": "../../package.json"
	}
}

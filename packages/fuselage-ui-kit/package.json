    "typescript": "~5.3.2"
  },
  "dependencies": {
    "@rocket.chat/gazzodown": "workspace:^",
    "@rocket.chat/ui-kit": "workspace:~",
    "tslib": "^2.5.3"
  },
  "volta": {
    "extends": "../../package.json"
  }

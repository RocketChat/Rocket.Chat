		"@rocket.chat/ui-video-conf": "workspace:^",
		"@tanstack/react-query": "*",
		"i18next": "*",
		"react": "*",
		"react-dom": "*",
		"react-i18next": "*"
	},
	"volta": {
		"extends": "../../package.json"

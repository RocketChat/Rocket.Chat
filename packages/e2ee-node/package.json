{
  "name": "@rocket.chat/e2ee-node",
  "private": true,
  "version": "0.1.0",
  "description": "Runtime-agnostic, sans-IO end-to-end encryption core for Rocket.Chat (Double Ratchet, key management, message formats)",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "postbuild": "node --experimental-strip-types ./scripts/postbuild.ts",
    "typecheck": "tsc -p tsconfig.json",
    "lint": "eslint 'src/**/*.ts'",
    "testunit": "vitest run"
  },
  "dependencies": {
    "@rocket.chat/e2ee": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "~22.16.5",
    "typescript": "~5.9.2",
    "vitest": "4.0.0-beta.8"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "extends": "../../package.json"
  }
}

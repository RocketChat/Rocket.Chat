v) {\n    Meteor = v;\n  }\n\n}, 0);\nlet Users, Rooms;\nmodule.link(\"../../../../models\", {\n  Users(v) {\n    Users = v;\n  },\n\n  Rooms(v) {\n    Rooms = v;\n  }\n\n}, 1);\n\nfunction handleOnCreateUser(newUser) {\n  if (!newUser) {\n    return this.log('Invalid handleOnCreateUser call');\n  }\n\n  if (!newUser.username) {\n    return this.log('Invalid handleOnCreateUser call (Missing username)');\n  }\n\n  if (this.loggedInUsers.indexOf(newUser._id) !== -1) {\n    return this.log('Duplicate handleOnCreateUser call');\n  }\n\n  this.loggedInUsers.push(newUser._id);\n  Meteor.users.update({\n    _id: newUser._id\n  }, {\n    $set: {\n      'profile.irc.fromIRC': false,\n      'profile.irc.username': \"\".concat(newUser.username, \"-rkt\"),\n      'profile.irc.nick': \"\".concat(newUser.username, \"-rkt\"),\n      'profile.irc.hostname': 'rocket.chat'\n    }\n  });\n  const user = Users.findOne({\n    _id: newUser._id\n  });\n  this.sendCommand('registerUser', user);\n  const rooms = Rooms.findBySubscriptionUserId(user._id).fetch();\n  rooms.forEach(room => this.sendCommand('joinedChannel', {\n    room,\n    user\n  }));\n}","map":{"version":3,"sources":["app/irc/server/irc-bridge/localHandlers/onCreateUser.js"],"names":["module","export","default","handleOnCreateUser","Meteor","link","v","Users","Rooms","newUser","log","username","loggedInUsers","indexOf","_id","push","users","update","$set","user","findOne","sendCommand","rooms","findBySubscriptionUserId","fetch","forEach","room"],"mappings":"AAAAA,MAAM,CAACC,MAAP,CAAc;AAACC,EAAAA,OAAO,EAAC,MAAIC;AAAb,CAAd;AAAgD,IAAIC,MAAJ;AAAWJ,MAAM,CAACK,IAAP,CAAY,eAAZ,EAA4B;AAACD,EAAAA,MAAM,CAACE,CAAD,EAAG;AAACF,IAAAA,MAAM,GAACE,CAAP;AAAS;;AAApB,CAA5B,EAAkD,CAAlD;AAAqD,IAAIC,KAAJ,EAAUC,KAAV;AAAgBR,MAAM,CAACK,IAAP,CAAY,oBAAZ,EAAiC;AAACE,EAAAA,KAAK,CAACD,CAAD,EAAG;AAACC,IAAAA,KAAK,GAACD,CAAN;AAAQ,GAAlB;;AAAmBE,EAAAA,KAAK,CAACF,CAAD,EAAG;AAACE,IAAAA,KAAK,GAACF,CAAN;AAAQ;;AAApC,CAAjC,EAAuE,CAAvE;;AAIjH,SAASH,kBAAT,CAA4BM,OAA5B,